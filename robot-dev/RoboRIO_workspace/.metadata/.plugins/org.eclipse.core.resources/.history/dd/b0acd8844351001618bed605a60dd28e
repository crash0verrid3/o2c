	#define ACTIVESENSE 1.0						// Spin Control correction factor.  Higher numbers correct more violently.
	#define SPIN_RATE_STABILITY_THRESHOLD 1.0	// uncommanded spin rate less than 1 dps will activate SpinControl
#endif


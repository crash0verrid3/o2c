		double	Nav_R_Wheel;
	#endif
	#ifdef BNO055_9DOF
		int		IMU_ChipID;
		int		IMU_CalStat;
		int		IMU_SelfTest;
		int		IMU_IntrStat;
		int		IMU_ClockStat;
		int		IMU_SystemStat;
